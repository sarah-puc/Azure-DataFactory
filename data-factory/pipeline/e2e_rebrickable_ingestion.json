{
	"name": "e2e_rebrickable_ingestion",
	"properties": {
		"activities": [
			{
				"name": "ac_set_desired_endpoints",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "endpoints",
					"value": {
						"value": "@pipeline().parameters.endpoints",
						"type": "Expression"
					}
				}
			},
			{
				"name": "ac_for_each_endpoint",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "ac_set_desired_endpoints",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@variables('endpoints')",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "ac_execute_copy_api_pipeline",
							"type": "ExecutePipeline",
							"dependsOn": [],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "p_copy_rebrickable_user_data",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"user_route": {
										"value": "@item()",
										"type": "Expression"
									},
									"user_email": {
										"value": "@pipeline().parameters.user",
										"type": "Expression"
									}
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"endpoints": {
				"type": "array"
			},
			"user": {
				"type": "string",
				"defaultValue": "r.sarah@pucpr.edu.br"
			}
		},
		"variables": {
			"endpoints": {
				"type": "Array"
			}
		},
		"folder": {
			"name": "Rebrickable"
		},
		"annotations": []
	}
}