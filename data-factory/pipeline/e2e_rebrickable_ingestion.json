{
	"name": "e2e_rebrickable_ingestion",
	"properties": {
		"activities": [
			{
				"name": "ac_set_desired_endpoints_users_api",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "endpoints",
					"value": {
						"value": "@pipeline().parameters.endpoints",
						"type": "Expression"
					}
				}
			},
			{
				"name": "ac_for_each_user_endpoint",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "ac_set_desired_endpoints_users_api",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@variables('endpoints')",
						"type": "Expression"
					},
					"isSequential": false,
					"activities": [
						{
							"name": "ac_execute_copy_api_pipeline",
							"type": "ExecutePipeline",
							"dependsOn": [],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "p_copy_rebrickable_user_data",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"user_route": {
										"value": "@item()",
										"type": "Expression"
									},
									"user_email": {
										"value": "@pipeline().parameters.user",
										"type": "Expression"
									}
								}
							}
						}
					]
				}
			},
			{
				"name": "ac_set_desired_download_datasets",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "download_datasets",
					"value": {
						"value": "@pipeline().parameters.downloads",
						"type": "Expression"
					}
				}
			},
			{
				"name": "ac_for_each_download_dataset",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "ac_set_desired_download_datasets",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@variables('download_datasets')",
						"type": "Expression"
					},
					"isSequential": false,
					"activities": [
						{
							"name": "ac_execute_copy_download_dataset",
							"type": "ExecutePipeline",
							"dependsOn": [],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "p_copy_rebrickable_zipped_dataset",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"dataset_name": {
										"value": "@item()",
										"type": "Expression"
									}
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"endpoints": {
				"type": "array",
				"defaultValue": [
					"minifigs",
					"partlists",
					"profile",
					"parts",
					"setlists"
				]
			},
			"user": {
				"type": "string",
				"defaultValue": "r.sarah@pucpr.edu.br"
			},
			"downloads": {
				"type": "array",
				"defaultValue": [
					"themes",
					"colors",
					"part_categories",
					"parts",
					"part_relationships",
					"elements",
					"sets",
					"minifigs",
					"inventories"
				]
			}
		},
		"variables": {
			"endpoints": {
				"type": "Array"
			},
			"download_datasets": {
				"type": "Array"
			}
		},
		"folder": {
			"name": "Rebrickable"
		},
		"annotations": []
	}
}