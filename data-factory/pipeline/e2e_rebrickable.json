{
	"name": "e2e_rebrickable",
	"properties": {
		"activities": [
			{
				"name": "ac_copy_zipped_content",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ds_generic_zipped_rebrickable_datasets",
						"type": "DatasetReference",
						"parameters": {
							"dataset": {
								"value": "@pipeline().parameters.dataset_name",
								"type": "Expression"
							},
							"extension": ".csv.gz"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_rebrickable_zipped_csv_file",
						"type": "DatasetReference",
						"parameters": {
							"dataset": {
								"value": "@pipeline().parameters.dataset_name",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"dataset_name": {
				"type": "string"
			}
		},
		"annotations": []
	}
}